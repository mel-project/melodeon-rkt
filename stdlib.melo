provide CoinID
provide CoinData
provide Transaction

struct CoinID {
	txhash: #[32],
	index: Nat
}

struct CoinData {
	covhash: #[32],
	value: Nat,
	denom: Nat,
	additional_data: Any,
}

struct Transaction {
	kind: Nat,
	inputs: [CoinID...],
	outputs: [CoinData...],
	fee: Nat,
	scripts: Any,
	data: Any,
	sigs: Any
}
